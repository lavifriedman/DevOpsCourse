def are_lists_equal(list1, list2):
    """function collect tow lists of int and float value's types,
    and checkif they contian the exactd smae values no mater they order.
    :param list1: list of int/float value's type that comper to list2.
    :param list2: list of int/float value's type that comper to list1.
    :type list1: list.
    :type list2: list.
    :return: True/False if the tow lists contian the exactd same values.
    :rtype: bool.
    """
    if sorted(list1) == sorted(list2):
        return True
    else:
        return False

def main():
    print(are_lists_equal.__doc__)
    list_1 = [1, 2, 3, 4, 5.5]
    list_2 = [3, 2, 1, 5.5, 4]
    list_3 = [1.2, 3, 5]
    print(are_lists_equal(list_1, list_2))
    print(are_lists_equal(list_1, list_3))
    
if __name__ == "__main__":
    main()